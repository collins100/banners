/*		end frame, CTA, and related styles		*/

#endFrame {
	position: absolute;
	left: 0;
	top: 0;
	width: 300px;
	height: 140px;
	opacity: 0;
}
#endFrame img {
	position: absolute;
	left: 0;
	top: 12px;
}
#endFrame p {
	position: absolute;
	left: 143px;
	top: 17px;
	display: block;
	width: 154px;
	font-size: 14px;
	line-height: 16px;
}

#cta {
	position: absolute;
	left: 143px;
	top: 80px;
	display: block;
	width: 134px;
	height: 28px;
	font-size: 12px;
	line-height: 22px;
	font-weight: bold;
	text-align: center;
	background: #ee8b1f;
	-webkit-border-radius: 5px;
	-moz-border-radius:    5px;
	border-radius:         5px;
}
#cta span {
	display: block;
	margin: 3px 0;
}

#bg-exit:hover #cta {
	background: #ee8b1f;
}


/*		individual styles and variations to slide contents		*/


#image1 img {
	margin-top: 13px;
}










