* {
	margin: 0;
	padding: 0;
	border: 0;
}
a,
.text_link {
	cursor: pointer;
	color: inherit;
	text-decoration: none;
}
h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
}

.banner_frame {
	float: left;
	position: relative;
	width: 300px;
	height: 250px;
	color: #fff;
	font-family:"Arial", sans-serif;
	border: solid 1px #000;
	overflow: hidden;
	box-sizing: border-box;
	position: relative;
	cursor: pointer;
	max-width: 300px;
	background: #ffffff;
}
#animatedCyanBox {
	background: #00a9ed;
	width: 300px;
	height: 250px;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 2;
}
#animatedBlueBox {
	background: #004986;
	width: 300px;
	height: 71px;
	position: absolute;
	top: 21px;
	left: 0px;
	opacity: 0;
	z-index: 1;
}
.mainFrame {
	display: table;
	opacity: 0;
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 10;
	width: 300px;
	box-sizing: border-box;
}
.centerVertical {
	display: table-cell;
	vertical-align: middle;
	width: 300px;
	box-sizing: border-box;
}
h1 {
	display: block;
	margin-left: 40px;
	font-size: 52px;
}
h2 {
	display: block;
	margin-left: 20px;
	font-size: 20px;
	line-height: 24px;
	text-transform:uppercase;
}
.center h2 {
	margin-left: 0px;
}
h3 {
	display: block;
	padding-left: 20px;
	padding-right: 20px;
	padding-top: 10px;
	padding-bottom: 10px;
	font-size: 20px;
	line-height: 24px;
	font-weight: bold;
	text-transform:uppercase;
}
h4 {
	display: block;
	padding-left: 20px;
	padding-right: 15px;
	padding-top: 15px;
	padding-bottom: 15px;
	font-size: 15px;
	line-height: 18px;
}
h1 span, h2 span, h3 span, h4 span {
	display: block;
}
sup {
	font-size: 50%;
}
#questionMark {
	position: absolute;
	top: 100px;
	right: 111px;
	z-index: 2;
	color: #ffffff;
	font-size: 52px;
	opacity: 0;
	font-family: "Arial Black", "Arial Bold", sans-serif
}
#HUH {
	font-family: "Arial Black", "Arial Bold", sans-serif;
	margin-left: 33px;
	margin-top: 5px;
}
#frame5, #frame6, #frame7 {
	opacity: 1;
}
#frame5textA, #frame5textB, #frame6logo, #frame6textA, #frame6textB, #frame6textC, #frame7headA, #frame7headB, #frame7headC, #frame7textA, #frame7textB, #CTAbutton, #frame7Logo, #disclaimer {
	opacity: 0;
}
#finalLogoCTA {
	padding: 10px;
}
#frame7Logo {
	position: relative;
	left: -10px;
}
#frame7head h3{
   font-size: 18px;
   line-height: 22px;
}
#CTAbutton {
	background-color: #014886;
	float: right;
	font-size: 14px;
	line-height: 34px;
	text-transform:uppercase;
	padding-left: 10px;
	padding-right: 10px;
}
#disclaimer {
	font-size: 8px;
	line-height: 9px;
	color: #58595b;
	margin-top: 4px;
}

/* ---- Generic Styles  -----  */
.height50Percent {
	height: 125px;
	box-sizing: border-box;
}
.height100Percent {
	height: 250px;
	box-sizing: border-box;
}
.whiteBkgd {
	background-color: #ffffff;
}
.darkBlueBkgd {
	background-color: #004986;
}
.cyanBkgd {
	background-color: #00a9ed;
}
.center {
	text-align: center;
}
.displayBlock {
	display: block;
	
}

